{
  "name": "@feature-flag/server",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "db:generate": "prisma generate --schema=src/prisma/schema.prisma",
    "db:migrate": "prisma migrate dev --schema=src/prisma/schema.prisma",
    "db:push": "prisma db push --schema=src/prisma/schema.prisma",
    "db:seed": "tsx src/prisma/seed.ts"
  },
  "dependencies": {
    "@feature-flag/shared": "*",
    "@prisma/client": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.0",
    "express": "^4.18.0",
    "jsonwebtoken": "^9.0.0",
    "uuid": "^9.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/uuid": "^9.0.7",
    "prisma": "^6.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.4.0"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  }
}
